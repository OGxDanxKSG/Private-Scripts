local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library//main/Library", true))()

local window = library:AddWindow("Muscle Legends V5 | Made By Ollie", {
	main_color = Color3.fromRGB(41, 74, 122), 
	min_size = Vector2.new(450, 450),
	can_resize = false, 
})

local Main = window:AddTab("Main")

Main:AddLabel("Player Settings")

Main:AddTextBox("Set Speed 1-500",function(text)
	
end)

Main:AddSwitch("Set Speed",function()

end)

local vu500 = v465:AddTab("Players")
    local vu501 = {}
    local vu502 = nil
    local vu503 = nil
    local function vu505(p504)
        if p504 >= 1000000000000000 then
            return string.format("%.1fQa", p504 / 1000000000000000)
        elseif p504 >= 1000000000000 then
            return string.format("%.1fT", p504 / 1000000000000)
        elseif p504 >= 1000000000 then
            return string.format("%.1fB", p504 / 1000000000)
        elseif p504 >= 1000000 then
            return string.format("%.1fM", p504 / 1000000)
        elseif p504 >= 1000 then
            return string.format("%.1fK", p504 / 1000)
        else
            return tostring(p504)
        end
    end
    local function vu520(pu506)
        local v507 = pu506.Name
        local vu508 = pu506:FindFirstChild("leaderstats")
        local v509 = pu506:FindFirstChild("equippedPets")
        local v510 = pu506:FindFirstChild("ownedGamepasses")
        local vu511 = {
            StrengthLabel = vu500:AddLabel("Strength: " .. vu505(vu508.Strength.Value or 0)),
            DurabilityLabel = vu500:AddLabel("Durability: " .. vu505(pu506.Durability.Value or 0)),
            KillsLabel = vu500:AddLabel("Kills: " .. vu505(vu508.Kills.Value or 0)),
            BrawlsLabel = vu500:AddLabel("Brawls: " .. vu505(vu508.Brawls.Value or 0)),
            AgilityLabel = vu500:AddLabel("Agility: " .. vu505(pu506.Agility.Value or 0)),
            EvilKarmaLabel = vu500:AddLabel("evilKarma: " .. vu505(pu506.evilKarma.Value or 0)),
            GoodKarmaLabel = vu500:AddLabel("goodKarma: " .. vu505(pu506.goodKarma.Value or 0)),
            MapLabel = vu500:AddLabel("Map: " .. (pu506.currentMap.Value or "N/A")),
            KingTimeLabel = vu500:AddLabel("KingTime: " .. vu505(pu506.muscleKingTime.Value or 0)),
            PremiumLabel = vu500:AddLabel("Premium: " .. (pu506.MembershipType == Enum.MembershipType.Premium and "true" or "false"))
        }
        for v512 = 1, 5 do
            local v513 = v509:FindFirstChild("pet" .. v512) and (v509["pet" .. v512].Value or "N/A") or "N/A"
            local v514 = vu500
            vu511["Pet" .. v512 .. "Label"] = v514:AddLabel("Pet" .. v512 .. ": " .. tostring(v513))
        end
        local v515 = {}
        if v510 then
            local v516, v517, v518 = ipairs(v510:GetChildren())
            while true do
                local v519
                v518, v519 = v516(v517, v518)
                if v518 == nil then
                    break
                end
                table.insert(v515, v519.Name)
            end
        end
        vu511.GamepassesLabel = vu500:AddLabel("ownedGamepasses: " .. (# v515 > 0 and table.concat(v515, ", ") or "N/A"))
        vu501[v507] = vu511
        vu508.Kills.Changed:Connect(function()
            vu511.KillsLabel.Text = "Kills: " .. vu505(vu508.Kills.Value or 0)
        end)
        vu508.Strength.Changed:Connect(function()
            vu511.StrengthLabel.Text = "Strength: " .. vu505(vu508.Strength.Value or 0)
        end)
        vu508.Brawls.Changed:Connect(function()
            vu511.BrawlsLabel.Text = "Brawls: " .. vu505(vu508.Brawls.Value or 0)
        end)
        pu506.Durability.Changed:Connect(function()
            vu511.DurabilityLabel.Text = "Durability: " .. vu505(pu506.Durability.Value or 0)
        end)
        pu506.Agility.Changed:Connect(function()
            vu511.AgilityLabel.Text = "Agility: " .. vu505(pu506.Agility.Value or 0)
        end)
        pu506.evilKarma.Changed:Connect(function()
            vu511.EvilKarmaLabel.Text = "evilKarma: " .. vu505(pu506.evilKarma.Value or 0)
        end)
        pu506.goodKarma.Changed:Connect(function()
            vu511.GoodKarmaLabel.Text = "goodKarma: " .. vu505(pu506.goodKarma.Value or 0)
        end)
        pu506.currentMap.Changed:Connect(function()
            vu511.MapLabel.Text = "Map: " .. (pu506.currentMap.Value or "N/A")
        end)
        pu506.muscleKingTime.Changed:Connect(function()
            vu511.KingTimeLabel.Text = "KingTime: " .. vu505(pu506.muscleKingTime.Value or 0)
        end)
    end
    local function vu527(p521)
        local v522 = vu501[p521]
        if v522 then
            local v523, v524, v525 = pairs(v522)
            while true do
                local v526
                v525, v526 = v523(v524, v525)
                if v525 == nil then
                    break
                end
                v526:Remove()
            end
            vu501[p521] = nil
        end
    end
    local v528 = vu500
    vu500.AddTextBox(v528, "Player Name", function(p529)
        local v530 = game.Players:FindFirstChild(p529)
        if vu503 then
            vu503:Remove()
            vu503 = nil
        end
        if v530 then
            if vu502 then
                vu527(vu502)
            end
            vu520(v530)
            vu502 = p529
        else
            if vu503 == nil then
                vu503 = vu500:AddLabel("Player not found!")
            end
            if vu502 then
                vu527(vu502)
            end
            vu502 = nil
        end
    end)
    game.Players.PlayerRemoving:Connect(function(p531)
        if vu502 == p531.Name then
            vu527(p531.Name)
            vu502 = nil
        end
    end)
    game.Players.PlayerAdded:Connect(function(_)
    end)
    local vu532 = nil
    local vu533 = nil
    local function vu542(p534)
        local v535, v536, v537 = ipairs(game.Players:GetPlayers())
        local v538 = nil
        while true do
            local v539
            v537, v539 = v535(v536, v537)
            if v537 == nil then
                v539 = v538
                break
            end
            if v539.Name:lower():match(p534:lower()) then
                break
            end
        end
        if v539 then
            local function v541(p540)
                game.Workspace.CurrentCamera.CameraSubject = p540
            end
            v541(v539.Character or v539.CharacterAdded:Wait())
            if vu532 then
                vu532:Disconnect()
                vu532 = nil
            end
            vu532 = v539.CharacterAdded:Connect(v541)
            vu533 = v539
        end
    end
    local v543 = vu500
    vu500.AddTextBox(v543, "View Player", function(p544)
        vu542(p544)
    end)
    local v545 = vu500
    vu500.AddButton(v545, "Unview Player", function()
        if vu532 then
            vu532:Disconnect()
            vu532 = nil
        end
        local v546 = game.Players.LocalPlayer
        game.Workspace.CurrentCamera.CameraSubject = v546.Character or v546.CharacterAdded:Wait()
        vu533 = nil
    end)
    local v547 = v465:AddTab("Leaderboard")
    v547:AddButton("Copy King LB", function()
        local v548 = game.Workspace.muscleKingLeaderboard.leaderboardPart.leaderboardGui.playerList.innerFrame
        if v548 then
            local v549, v550, v551 = pairs(v548:GetChildren())
            local v552 = {}
            local v553 = "[\"muscleKingLeaderboard\"] = {\n"
            while true do
                local v554
                v551, v554 = v549(v550, v551)
                if v551 == nil then
                    break
                end
                local v555, v556, v557 = pairs(v554:GetChildren())
                local v558 = ""
                local v559 = ""
                local v560 = ""
                while true do
                    local v561
                    v557, v561 = v555(v556, v557)
                    if v557 == nil then
                        break
                    end
                    if v561:IsA("TextLabel") then
                        if v561.Name ~= "nameLabel" then
                            if v561.Name ~= "amountLabel" then
                                if v561.Name == "rankLabel" then
                                    v560 = v561.Text
                                end
                            else
                                v559 = v561.Text
                            end
                        else
                            v558 = v561.Text
                        end
                    end
                end
                if v558 ~= "" and (v559 ~= "" and v560 ~= "") then
                    table.insert(v552, {
                        v558,
                        v559,
                        v560
                    })
                end
            end
            local v562, v563, v564 = pairs(v552)
            while true do
                local v565
                v564, v565 = v562(v563, v564)
                if v564 == nil then
                    break
                end
                local v566 = v553 .. "  {\"" .. v565[1] .. "\", \"" .. v565[2] .. "\", \"" .. v565[3] .. "\"}"
                if v564 >= # v552 then
                    v553 = v566 .. "\n"
                else
                    v553 = v566 .. ",\n"
                end
            end
            local v567 = v553 .. "},"
            setclipboard(v567)
        end
    end)
